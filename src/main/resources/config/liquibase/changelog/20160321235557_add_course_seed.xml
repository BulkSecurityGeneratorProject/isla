<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
  <changeSet id="20160321235557" author="">
        <loadData encoding="UTF-8"
                  file="config/liquibase/courses.csv"
                  separator=";"
                  tableName="COURSE">
            <column name="id" type="numeric"/>
            <column name="course_name" type="string"/>
            <column name="course_description" type="string"/>
        </loadData>
        <loadData encoding="UTF-8"
                  file="config/liquibase/course_moderators.csv"
                  separator=";"
                  tableName="COURSE_MODERATORS">
            <column name="course_id" type="numeric"/>
            <column name="user_id" type="numeric"/>
        </loadData>
  </changeSet>
</databaseChangeLog>
